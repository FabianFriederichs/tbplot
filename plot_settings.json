{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "width" : 20.0,
    "height" : 12.0,
    "fontsize" : {
        "plot_title" : 9.0,
        "axis_label" : 8.0,
        "axis_tick_major" : 8.0,
        "axis_tick_minor" : 6.0,
        "legend" : 8.0
    },    
    "legend" : true,
    "linestyles" : {
        "primary" : {
            "width" : 1.0,
            "style" : "solid",
            "alpha" : 1.0
        },
        "secondary" : {
            "width" : 1.0,
            "style" : "dashed",
            "alpha" : 0.5
        }
    },
    "tick_style_x" : "plain",
    "tick_style_y" : "plain",
    "tick_sci_limits" : {
        "x" : [-2.0, 2.0],
        "y" : [-2.0, 2.0]
    }
}